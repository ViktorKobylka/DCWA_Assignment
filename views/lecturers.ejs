<h1>Lecturers</h1><br>
<a href="/">Home</a>
<table>
    <thead>
        <tr>
            <th>Lecturer ID</th>
            <th>Name</th>
            <th>Department ID</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% lecturers.forEach(lecturer => { %>
            <tr>
                <td><%= lecturer._id %></td>
                <td><%= lecturer.name %></td>
                <td><%= lecturer.did %></td>
                <td><a href="/lecturers/delete/<%= lecturer._id %>">Delete</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>